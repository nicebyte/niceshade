using namespace metal;

struct PSMain_out
{
    float4 out_var_SV_TARGET [[color(0)]];
};

fragment PSMain_out PSMain(float4 gl_FragCoord [[position]])
{
    PSMain_out out = {};
    out.out_var_SV_TARGET = (gl_FragCoord * 0.5) + float4(0.5);
    return out;
}

